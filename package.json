{
  "name": "flash-loan-arbitrage-bot",
  "version": "2.0.0",
  "description": "Production-grade Flash Loan Arbitrage Bot V2 for Arbitrum",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index-real-bot.js",
    "start:old": "node dist/index-production.js",
    "dev": "ts-node src/index-real-bot.ts",
    "compile": "hardhat compile",
    "clean": "hardhat clean && rm -rf dist",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "deploy": "hardhat run scripts/deployV2.ts --network arbitrum",
    "deploy:testnet": "hardhat run scripts/deployV2.ts --network arbitrum-sepolia",
    "deploy:local": "hardhat run scripts/deployV2.ts --network localhost",
    "verify": "hardhat verify --network arbitrum",
    "security-audit": "./scripts/security-audit.sh",
    "key-manager": "ts-node src/security/SecureKeyManager.ts",
    "key:encrypt": "ts-node src/security/SecureKeyManager.ts encrypt",
    "key:migrate": "ts-node src/security/SecureKeyManager.ts migrate",
    "key:verify": "ts-node src/security/SecureKeyManager.ts verify",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "size": "hardhat size-contracts"
  },
  "keywords": ["arbitrage", "flash-loan", "defi", "arbitrum"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@uniswap/v3-sdk": "^3.11.2",
    "@uniswap/sdk-core": "^4.2.1",
    "@uniswap/v2-sdk": "^3.2.2",
    "@flashbots/ethers-provider-bundle": "^0.6.2",
    "ethers": "^5.7.2",
    "node-telegram-bot-api": "^0.64.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "bignumber.js": "^9.1.2",
    "ws": "^8.15.0",
    "decimal.js": "^10.4.3",
    "fp-ts": "^2.16.0",
    "io-ts": "^2.2.20"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/ws": "^8.5.10",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "hardhat": "^2.19.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@typechain/ethers-v5": "^11.1.2",
    "@typechain/hardhat": "^9.1.0",
    "typechain": "^8.3.2",
    "@openzeppelin/contracts": "^4.9.5",
    "chai": "^4.3.10",
    "ethereum-waffle": "^4.0.10",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "solhint": "^4.0.0",
    "solidity-coverage": "^0.8.5"
  }
}
